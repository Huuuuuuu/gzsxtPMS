    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>


<!-- Mirrored from www.gzsxt.cn/theme/hplus/register.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 20 Jan 2016 14:19:52 GMT -->
<head>
	<base href="<%=basePath%>">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <title>绿地中央广场综合物业办公系统 - 注册</title>
    <meta name="keywords" content="绿地中央广场综合物业办公系统">
    <meta name="description" content="绿地中央广场综合物业办公系统">

    <link rel="shortcut icon" href="favicon.ico"> <link href="css/bootstrap.min14ed.css?v=3.3.6" rel="stylesheet">
    <link href="css/font-awesome.min93e3.css?v=4.4.0" rel="stylesheet">
    <link href="css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="css/animate.min.css" rel="stylesheet">
    <link href="css/style.min862f.css?v=4.1.0" rel="stylesheet">
    <link href="css/plugins/sweetalert/sweetalert.css" rel="stylesheet">
    <link href="css/animate.min.css" rel="stylesheet">
    <link href="css/style.min862f.css?v=4.1.0" rel="stylesheet">
    <script>if(window.top !== window.self){ window.top.location = window.location;}</script>

</head>

<body class="gray-bg">

    <div class="middle-box loginscreen   animated fadeInDown">
       
            
            <form id="regt"  class="m-t" role="form" action="regservlet"  method="post"  onsubmit="return registerallinfo()">
            	
            	<div class="form-group">
                    <input id="name"  name="name" type="text" class="form-control" placeholder="请输入姓名" required=""  onblur="checkName(),cdbname()">
                    <span id="info1"><i class="fa fa-info-circle"></i>请输入真实姓名</span>
                </div>
                <div class="form-group">
                    <input id="username" name="username"  type="text" class="form-control" placeholder="请输入用户名" required=""  onblur="checkUserName()">
                    <span id="info2"><i class="fa fa-info-circle"></i>请输入至少4个字符，至多16个字符</span>
                </div>
                <div class="form-group">
                    <input id="password" name="password"   type="password" class="form-control" placeholder="请输入密码" required=""  onblur="checkPassWord()">
                    <span id=info3><i class="fa fa-info-circle"></i> 请输入由字母和数字组成并以字母开头6至18位密码</span>
                </div>
                <div class="form-group">
                    <input id="password2"  name="password2"  type="password" class="form-control" placeholder="请再次输入密码" required=""  onblur="checkPassWord2()">
                    <span id="info4"><i class="fa fa-info-circle"></i> 请再次输入密码</span>
                </div>
                
                <div class="form-group">
                    <input id="phone" name="phone"  type="text" class="form-control" placeholder="请输入手机号码" required=""  onblur="checkPhone()">
                    <span id="info5"><i class="fa fa-info-circle"></i> 请输入手机号码</span>
                </div>
                
                <div class="form-group">
                    <input id="email" name="email"  type="text" class="form-control" placeholder="请输入邮箱" required=""  onblur="checkEmail()">
                    <span id="info6"><i class="fa fa-info-circle"></i> 请输入邮箱</span>
                </div>
  
                
                    <div>
                         <label > 
                            <input type="checkbox" id="zcbox"  onclick="checkButton()">我同意注册协议</label> 
                	</div>
              
                <button id="bzc" type="button" class="btn btn-primary block full-width m-b" disabled="disabled"  onclick="registerAtrly()">注 册</button>

                <p class="text-muted text-center"><small>已经有账户了？</small><a href="page/login.jsp">点此登录</a>
                </p>

            </form>
        </div>
    </div>
    <script src="js/jquery.min.js?v=2.1.4"></script>
    <script src="js/bootstrap.min.js?v=3.3.6"></script>
    <script src="js/plugins/iCheck/icheck.min.js"></script>
    <script>
        $(document).ready(function(){$(".i-checks").iCheck({checkboxClass:"icheckbox_square-green",radioClass:"iradio_square-green",})});
    </script>
    <script type="text/javascript" src="http://tajs.qq.com/stats?sId=9051096" charset="UTF-8"></script>
    
    <script type="text/javascript">
    
    
    /*注册全信息检查*/
    function registerallinfo() {
    	var nf = checkName()
        var unf = checkUserName()
        var pwdf = checkPassWord()
        var pwd2f = checkPassWord2()
        var phef = checkPhone()
        var ef = checkEmail()
        
        console.log(unf&&pwdf&&pwd2f&&phef&&ef)
        if(unf&&pwdf&&pwd2f&&phef&&ef){
        	/* swal({
            	title:"注册成功",
            	type:"success",
            })  */
            return true
        	
        }else{
            /* alert("注册信息未按要求填写") */
            swal({
            	title:"格式错误",
        		text:"您没有按照要求填写注册信息，请根据提示要求填写注册信息",
        		type:"warning",
            })
            return false
        }
    }
    
    /*检查输入*/
    function checkinput(objid,spanid,reg,errMsg) {
        var Node = $(objid)
        var spanNode = $(spanid)

        var str = Node.val()
        if(reg.test(str)){
            spanNode.html(" ")
            spanNode.css("color","#1ab394")
            Node.css({"color":"#3c763d","border":"1px solid #1ab394"})
            return true
        }else {
            spanNode.html('<i class="fa fa-times-circle"></i>'+errMsg)
            spanNode.css("color","#a94442")
            Node.css({"color":"#a94442","border":"1px solid #ed5565"})
            return false
        }
    }
    
    
    /*检查用户名*/
    function checkName() {
        return checkinput("#name","#info1",/^[\u4e00-\u9fa5]{2,4}$/,"请输入中文，2到4位的真实姓名！")
    }
    
    
    /*检查用户名是否重复*/
    function cdbname() {
        	if(checkName()){
        		/* 获取name的值 */
        		var Node = $("#name")
        		var spanNode = $("#info1")
        		
        		var namestr = Node.val()
        		
        		/* ajax进行验证 */
        		$.ajax({
        			type :	"post",
        			url	:	"http://localhost:8080/gzsxt/cnservlet",
        			async 	:	true,
        			data		:	{"name":namestr},
        			dataType	:	"text",
        			success		:	function (data) {
        				if(data=="1" ){
        					spanNode.html('<i class="fa fa-times-circle"></i>'+"该用户已注册！")
        					spanNode.css("color","#a94442")
                    		Node.css({"color":"#a94442","border":"1px solid #ed5565"})
        				}
        			}
        		})
        	}
    	}
        
    /*检查用户名*/
    function checkUserName() {
	   return checkinput("#username","#info2",/^[a-zA-Z0-9_]{4,16}|[\u4e00-\u9fa5]{2,16}$/,"请输入至少4个字符，之多16个字符的用户名！")
	}
   

    /*检查密码*/
    function checkPassWord() {
	   return checkinput("#password","#info3",/^[a-zA-Z]\w{5,17}$/,"请输入由字母和数字组成的，以字母开头的6至18位的密码！")
	}

    /*再次确认密码*/
    function checkPassWord2() {
	        if(checkPassWord()){
	            var Node1 =  $("#password")
	            var Node2 = $("#password2")
	            var Node2span = $("#info4")

	            var pwd = Node1.val()
	            var pwd2 = Node2.val()
	            if (pwd==pwd2){
	                Node2span.html(" ")
	                Node2span.css("color","#1ab394")
	            	Node2.css({"color":"#3c763d","border":"1px solid #1ab394"})
	                return true
	            }else{
	                Node2span.html('<i class="fa fa-times-circle"></i>'+"两次不同密码相同！") 
	                Node2span.css("color","#a94442")
	                Node2.css({"color":"#a94442","border":"1px solid #ed5565"})
	                return false
	            }
	        }else{
	            return checkinput("#password2","#info4",/^[a-zA-Z]\w{5,17}$/,"请输入由字母和数字组成的，以字母开头的6至18位的密码！")
	        }
	    }

    /*检查手机号*/
    function checkPhone() {
	   return checkinput("#phone","#info5",/^1(3|4|5|7|8)\d{9}$/,"手机号码不正确！")
	}
    /*检查邮箱*/
   	function checkEmail() {
	    return checkinput("#email","#info6",/^[A-Za-z0-9\u4e00-\u9fa5]+@[a-zA-Z0-9_-]+(\.[a-zA-Z0-9_-]+)+$/,"请输入正确的邮箱！")
	}
    
    /*检测是否同意注册协议，注册按钮才能点击*/
    function checkButton() {
		if($("#zcbox").get(0).checked){
			$("#bzc").removeAttr("disabled")
		}else{
			$("#bzc").attr("disabled","disabled")
		}
	}
    
    function registerAtrly(){
    	swal({
            title: "您确定要注册这些信息吗",
            text: "注册后将无法更改，是否确定注册？",
            showCancelButton: true,
            confirmButtonColor: "#DD6B55",
            confirmButtonText: "注册",
            closeOnConfirm: false
        }, function () {
            $("#regt").submit();
        })
    }
   
  
    </script>
    <script src="js/plugins/sweetalert/sweetalert.min.js"></script>
    
    
    
